{
  "name": "double-track-browser",
  "version": "0.1.0",
  "description": "Privacy through deliberate visibility - create fictional browsing patterns alongside your real activity",
  "main": "dist/background.js",
  "scripts": {
    "build": "npm run build:rust && npm run build:extension",
    "build:rust": "cd rust_core && wasm-pack build --target web",
    "build:extension": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "clean": "rm -rf dist/ rust_core/pkg/",
    "test": "npm run test:rust && npm run test:ts",
    "test:rust": "cd rust_core && cargo test",
    "test:ts": "jest",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "privacy",
    "browser-extension",
    "webextension",
    "tracking-protection",
    "rust",
    "wasm"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {}
}
